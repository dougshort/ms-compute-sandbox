# {{ ansible_managed }}
{% for option in resolv_conf_options %}
{% if option %}
option {{ option }}
{% endif %}
{% endfor %}
{% for search_domain in resolv_conf_search_domains %}
{% if search_domain %}
search {{ search_domain }}
{% endif %}
{% endfor %}
{% for nameserver in resolv_conf_nameservers %}
nameserver {{ nameserver }}
{% endfor %}
